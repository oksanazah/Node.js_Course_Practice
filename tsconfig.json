{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "lib": ["es6"],
    "module": "commonjs",
    "baseUrl": ".", 
    "resolveJsonModule": true,
    "allowJs": true, 
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
    "exclude":[
      "./node_modules"
  ]
}
